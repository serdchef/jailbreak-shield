{
  "buildCommand": "cd demo && pip install -r requirements.txt",
  "outputDirectory": "demo",
  "env": {
    "ANTHROPIC_API_KEY": "@anthropic_api_key",
    "STREAMLIT_SERVER_HEADLESS": "true",
    "STREAMLIT_SERVER_ENABLE_XSRF_PROTECTION": "false"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
