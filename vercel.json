{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "cd web && npm install",
  "buildCommand": "cd web && npm run build",
  "outputDirectory": "web/.next",
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    }
  ]
}